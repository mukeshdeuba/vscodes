
#include<stdio.h>
#include<conio.h>
#include<conio.h>
int main()
{
int mat1[2][2]={{2,2},{3,3}};
int mat2[2][2]={{3,3},{2,2}};
int mat3[2][2];
for (int i = 0; i < 2; i++)
{
   for (int j= 0; j < 2; j++)
{
    mat3[i][j]=mat1[i][j]+mat2[i][j];
    int* ww=&mat3[i][j];
    printf("%d",*ww);
    printf("\t");
} 
printf("\n");
}
getch();
return 0;

}
